<?xml version="1.0" ?>
<launch>

    <param name="use_sim_time" value="true"/>
    <node pkg="rosbag" type="play" name="player" args="--clock /home/demo1/Desktop/robot_arm_3d_scan/2023-11-17-21-01-31.bag"/>

    <arg name="ground_removal" default="true"/>

    <rosparam command="load" file="$(find robot_arm_3d_scan)/config/AcquisitionParameters.yaml"/>

    <param name="measurementServerName" value="/point_cloud_server"/>
    <node pkg="robot_arm_3d_scan" type="point_cloud_server" name="point_cloud_server" output="screen">
        <param name="ground_removal" value="$(arg ground_removal)"/>
    </node>

</launch>
